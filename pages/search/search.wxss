.iconfont {
  font-family: "iconfont" !important;
  font-size: 16px;
  font-style: normal;
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
}

.icon-shanchu:before {
  content: "\e78d";
}

.icon-jiantou:before {
  content: "\e63f";
}


.searchlist{
  display: flex;
  padding: 30rpx;
  flex-direction: column;
}
.searchitem{
  width: 100%;
  display: flex;
  padding-bottom: 10rpx;
  border-bottom: 1px solid #f5f5f5;
}
.searchlist .searchimg image{
  width: 70rpx;
  height: 100rpx;
}
.searchtext{
  margin-left: 20rpx;
  display: flex;
  flex-direction: column;
  justify-content: space-between;
  font-size: 28rpx;
  color: #b3b3b3;
}
.history-title{
  display: flex;
  align-items: center;
  justify-content: space-between;
}
.history-title text:first-child{
  color: #b3b3b3;
  font-size: 28rpx;
}
.history-title text:last-child{
  color: #b3b3b3;
  font-size: 33rpx;
}
.historysearch{
  padding: 20rpx 30rpx;
}
.history-list{
  border-bottom: 1px solid #f5f5f5;
  width: 100%;
  padding-bottom: 10rpx;
  font-size: 28rpx;
  margin-top: 10rpx;
  color: #b3b3b3;
}

.dialog--weui-dialog__bd{
  margin-bottom: 20rpx !important;
  overflow-y: hidden !important;
}